{
  "extends": "@balena/lint/tsconfig.json",
  "compilerOptions": {
    "target": "es2020",
    "noImplicitAny": false,
    "allowJs": false,
    "outDir": "src",
    "esModuleInterop": true,
    "removeComments": false,
    "resolveJsonModule": true,
    "strict": true,
    "declaration": true,
    "skipLibCheck": true
  },
  "include": [
    "lib/**/*.ts"
  ]
}
