    "@types/lodash": "^4.14.183",
    "@types/uuid": "^8.3.4",
    "autumndb": "^22.1.14",
    "deplint": "^1.1.3",
    "hubot": "^3.3.2",
    "lint-staged": "^13.0.3",
    "rimraf": "^3.0.2",
    "simple-git-hooks": "^2.8.0",
